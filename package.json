{
  "name": "zip-to-tar",
  "version": "1.1.0",
  "description": "convert tar and tar.gz archives to zip",
  "main": "lib/zip-to-tar.js",
  "bin": {
    "zip2tar": "bin/zip-to-tar.js",
    "zip-to-tar": "bin/zip-to-tar.js",
    "zip2tar_": "legacy/bin/zip-to-tar.js",
    "zip-to-tar_": "legacy/bin/zip-to-tar.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel lib -d legacy/bin",
    "wisdom": "npm run build",
    "build": "redrun 6to5:* legacy:*",
    "legacy:index": "echo \"module.exports = require('./lib/zip-to-tar');\" > legacy/index.js",
    "legacy:package": "echo \"module.exports = require('../package');\" > legacy/package.js",
    "legacy:help": "echo \"module.exports = require('../help');\" > legacy/help.js",
    "lint:eslint": "eslint lib",
    "lint:eslint:bin": "eslint bin --config .eslint-bin.rc",
    "lint": "redrun lint:*",
    "test": "tape 'test/**/*.js'",
    "coverage": "nyc npm test",
    "coverage:summary": "nyc --reporter=text-summary npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:lint": "npm run watcher -- npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/node-zip-to-tar"
  },
  "keywords": [
    "tar",
    "gzip",
    "zip",
    "convert",
    "archive",
    "filter",
    "pipe",
    "stream",
    "event",
    "emitter",
    "progress"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "gunzip-maybe": "^1.3.1",
    "pipe-io": "^1.2.5",
    "tar-stream": "^1.5.2",
    "yauzl": "^2.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.9.1",
    "nodemon": "^1.11.0",
    "nyc": "^8.4.0",
    "redrun": "^5.9.3",
    "tape": "^4.6.2"
  }
}
